  "keywords": [
    "time",
    "tracker"
  ],
  "license": "GPL-3.0",
  "name": "timey",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/NiceOrg/timey.git"
  },
  "scripts": {
    "build": "nuxt generate",
    "check": "repo-check",
    "ci": "run-s build lint check test",
    "dev": "nuxt",
    "lint": "run-s lint:*",
    "lint:js": "eslint --fix --ignore-path .gitignore --ext \".js,.vue\" .",
    "lint:css": "stylelint \"{assets,components,layouts,pages}/**/*.{css,vue}\" --fix",
    "postversion": "git push && git push --tags",
    "preversion": "npm run ci",
    "start": "npm run dev",
    "test": "nyc mocha",
    "test:watch": "nodemon --quiet --watch services --watch tests --exec npm test --ext .ts",
    "update": "npx npm-check-updates -u"
  },
  "version": "1.0.0"
}
